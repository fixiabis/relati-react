{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "jsdoc-format": true,
        "jsx-wrap-multiline": false,
        "prefer-const": false,
        "trailing-comma": false,
        "curly": false,
        "jsx-no-lambda": false,
        "interface-name": false,
        "arrow-parens": false,
        "no-this-assignment": false,
        "max-classes-per-file": false,
        "no-bitwise": false,
        "no-console": false,
        "unified-signatures": false
    }
}